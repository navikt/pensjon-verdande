{
  "name": "pensjon-verdande",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "react-router build",
    "build:server": "tsc --project server/tsconfig.json",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "dev": "react-router dev",
    "dev:q2": "PEN_URL=https://pensjon-pen-q2.dev.adeo.no react-router dev",
    "fetch-secrets": "./fetch-secrets.sh",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "start": "react-router-serve ./build/server/index.js",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "build-storybook": "STORYBOOK=true storybook build",
    "capture-screenshots": "npx tsx scripts/capture-screenshots.ts",
    "capture-docs-screenshots": "npx tsx scripts/capture-screenshots.ts --docs",
    "test": "vitest --project unit",
    "test:stories": "vitest --project storybook",
    "typecheck": "node scripts/copy-server-dts.ts && react-router typegen && tsc"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@mjackson/form-data-parser": "^0.9.1",
    "@navikt/aksel-icons": "^8.5.2",
    "@navikt/ds-css": "^8.5.2",
    "@navikt/ds-react": "^8.5.2",
    "@react-router/express": "7.13.0",
    "@react-router/node": "7.13.0",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "compression": "^1.8.1",
    "date-fns-tz": "^3.2.0",
    "express": "^5.2.1",
    "isbot": "^5.1.35",
    "jwt-decode": "^4.0.0",
    "node-cache": "^5.1.2",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.4",
    "react-router": "7.13.0",
    "remix-auth": "^4.2.0",
    "remix-auth-oauth2": "^3.4.1",
    "tiny-invariant": "^1.3.3",
    "winston": "^3.19.0",
    "xml-formatter": "^3.6.7",
    "zod": "^4.3.6",
    "zod-form-data": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@react-router/dev": "7.13.0",
    "@storybook/addon-vitest": "^10.2.10",
    "@storybook/react": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "lefthook": "^2.1.1",
    "playwright": "^1.58.2",
    "react-router-devtools": "^6.2.0",
    "storybook": "^10.2.10",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=24.11.0"
  }
}
