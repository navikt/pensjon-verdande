    "lefthook": "^2.0.13",
    "msw": "^2.12.7",
    "react-router-devtools": "^6.0.2",
    "storybook": "^10.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
